
.background{
		position: fixed;
		width: 750rpx;
		height: var(--7aae1bf8);
		background-color: #fafafa;
}
.AppointmentBlock{
		width: 700rpx;
		background-color: white;
		border-radius: 30rpx;
		margin-left: 25rpx;
		margin-top: 50rpx;
		border: 1px solid #f0f0f0;
}
.title{
		display: flex;
		flex-direction: row;
		margin-top: 10rpx;
		margin-left: 10rpx;
